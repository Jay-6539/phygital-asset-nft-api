<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!-- Minimal Launch Screen with white background and centered image named 'LaunchLogo' (imageset) -->
<document type="com.apple.InterfaceBuilder3.CocoaTouch.Storyboard.XIB" version="3.0" toolsVersion="21507" targetRuntime="iOS.CocoaTouch" propertyAccessControl="none" useAutolayout="YES" launchScreen="YES" initialViewController="ViewController">
    <device id="retina6_12" orientation="portrait" appearance="light" />
    <scenes>
        <scene sceneID="launchScene">
            <objects>
                <viewController id="ViewController" sceneMemberID="viewController">
                    <view key="view" contentMode="scaleToFill" id="mainView">
                        <rect key="frame" x="0.0" y="0.0" width="390" height="844"/>
                        <color key="backgroundColor" white="1" alpha="1" colorSpace="custom" customColorSpace="genericGamma22GrayColorSpace"/>
                        <subviews>
                            <!-- LOGO图片，位置和大小与SplashOverlayView一致 -->
                            <imageView clipsSubviews="YES" userInteractionEnabled="NO" contentMode="scaleAspectFit" translatesAutoresizingMaskIntoConstraints="NO" image="LaunchLogo" id="logoImage"/>
                        </subviews>
                        <constraints>
                            <!-- LOGO水平居中 -->
                            <constraint firstItem="logoImage" firstAttribute="centerX" secondItem="mainView" secondAttribute="centerX" id="logoCenterX"/>
                            <!-- LOGO垂直居中 -->
                            <constraint firstItem="logoImage" firstAttribute="centerY" secondItem="mainView" secondAttribute="centerY" id="logoCenterY"/>
                            <!-- 宽度=40%屏宽，高度=20%屏高，与SplashOverlayView的fixedSide计算完全一致 -->
                            <constraint firstItem="logoImage" firstAttribute="width" secondItem="mainView" secondAttribute="width" multiplier="0.4" priority="999" id="logoWidthTarget"/>
                            <constraint firstItem="logoImage" firstAttribute="height" secondItem="mainView" secondAttribute="height" multiplier="0.2" priority="999" id="logoHeightTarget"/>
                            <!-- 宽度<=40%屏宽，高度<=20%屏高，确保不超出 -->
                            <constraint firstItem="logoImage" firstAttribute="width" relation="lessThanOrEqual" secondItem="mainView" secondAttribute="width" multiplier="0.4" id="logoWidthMax"/>
                            <constraint firstItem="logoImage" firstAttribute="height" relation="lessThanOrEqual" secondItem="mainView" secondAttribute="height" multiplier="0.2" id="logoHeightMax"/>
                            <!-- 保持宽高比1:1，最高优先级 -->
                            <constraint firstItem="logoImage" firstAttribute="width" secondItem="logoImage" secondAttribute="height" multiplier="1:1" priority="1000" id="logoAspect"/>
                        </constraints>
                    </view>
                </viewController>
                <placeholder placeholderIdentifier="IBFirstResponder" id="firstResponder" userLabel="First Responder" sceneMemberID="firstResponder"/>
            </objects>
        </scene>
    </scenes>
    <resources>
        <image name="LaunchLogo"/>
    </resources>
</document>
